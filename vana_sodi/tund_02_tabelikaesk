CREATE TABLE `juhokalberg`.`v21_news` ( `vr21_news_id` INT(11) NOT NULL AUTO_INCREMENT , `vr21_news_title` VARCHAR(150) CHARACTER SET utf8 COLLATE utf8_estonian_ci NOT NULL , `vr21_news_content` VARCHAR(1500) CHARACTER SET utf8 COLLATE utf8_estonian_ci NOT NULL , `vr21_news_author` VARCHAR(100) CHARACTER SET utf8 COLLATE utf8_estonian_ci NULL , `vr21_news_added` DATETIME NOT NULL DEFAULT CURRENT_TIMESTAMP , `vr21_news_deleted` DATETIME NULL , PRIMARY KEY (`vr21_news_id`)) ENGINE = InnoDB;